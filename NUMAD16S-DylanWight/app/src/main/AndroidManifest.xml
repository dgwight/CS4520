<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="dylanwight.madcourse.neu.edu.numad16s_dylanwight">
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>

    <application android:name=".mainMenu.NUMAD16s_DylanWight"
        android:largeHeap="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">

        <activity
            android:name=".mainMenu.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".tictactoe.TicTacToeActivity">
        </activity>
        <activity
            android:name=".wordDictionary.TestDictionaryActivity">
        </activity>
        <activity
            android:name=".tictactoe.GameActivity">
        </activity>

    </application>

</manifest>
